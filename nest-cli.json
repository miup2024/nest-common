{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": false,
    "webpack": false,
    "assets": [
      {
        "include": "**/*.env",
        "watchAssets": true,
        "outDir": "./dist/src"
      }
    ]
  },
  "projects": {
    "nest-oauth": {
      "type": "library",
      "root": "libs/nest-oauth",
      "entryFile": "index",
      "sourceRoot": "libs/nest-oauth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/nest-oauth/tsconfig.lib.json"
      }
    },
    "nest-config": {
      "type": "library",
      "root": "libs/nest-config",
      "entryFile": "index",
      "sourceRoot": "libs/nest-config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/nest-config/tsconfig.lib.json"
      }
    },
    "nest-log4j": {
      "type": "library",
      "root": "libs/nest-log4j",
      "entryFile": "index",
      "sourceRoot": "libs/nest-log4j/src",
      "compilerOptions": {
        "tsConfigPath": "libs/nest-log4j/tsconfig.lib.json"
      }
    },
    "nest-ali-sms": {
      "type": "library",
      "root": "libs/nest-ali-sms",
      "entryFile": "index",
      "sourceRoot": "libs/nest-ali-sms/src",
      "compilerOptions": {
        "tsConfigPath": "libs/nest-ali-sms/tsconfig.lib.json"
      }
    },
    "nest-redis": {
      "type": "library",
      "root": "libs/nest-redis",
      "entryFile": "index",
      "sourceRoot": "libs/nest-redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/nest-redis/tsconfig.lib.json"
      }
    },
    "nest-transaction": {
      "type": "library",
      "root": "libs/nest-transaction",
      "entryFile": "index",
      "sourceRoot": "libs/nest-transaction/src",
      "compilerOptions": {
        "tsConfigPath": "libs/nest-transaction/tsconfig.lib.json"
      }
    }
  }
}